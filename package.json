{
    "name": "getting-started-react",
    "version": "1.0.1",
    "description": "Get started with IronCore to learn how to grant, monitor and revoke access to sensitive data from a ReactJS application.",
    "author": "IronCore Labs",
    "homepage": "https://github.com/IronCoreLabs/getting-started-react",
    "repository": {
        "type": "git",
        "url": "https://github.com/IronCoreLabs/getting-started-react.git"
    },
    "license": "MIT",
    "scripts": {
        "insecure": "node src/server.js &! react-scripts start",
        "secure": "node src/server.js &! REACT_APP_SECURE=true react-scripts start"
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@ironcorelabs/ironweb": "^3.4.1",
        "es6-promise": "^4.2.8",
        "jsonwebtoken": "^8.5.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-redux": "^7.2.4",
        "react-scripts": "4.0.3",
        "redux": "^4.1.0",
        "redux-thunk": "^2.3.0",
        "whatwg-fetch": "^3.6.2"
    },
    "prettier": {
        "printWidth": 160,
        "tabWidth": 4,
        "trailingComma": "es5",
        "bracketSpacing": false,
        "jsxBracketSameLine": true,
        "arrowParens": "always"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">1.0%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
